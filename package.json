{
	"name": "shop-task",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"clean": "rimraf \"./.next\"",
		"dev": "yarn run clean && next dev",
		"build": "yarn run clean && next build",
		"start": "next start",
		"prettier:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
		"prettier:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
		"eslint:check": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
		"eslint:fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
		"spell": "cspell --no-progress \"src/**/*\"",
		"pre-commit": "yarn run prettier:check && yarn run eslint:check && yarn run spell && next lint",
		"lint": "yarn run pre-commit",
		"prepare": "husky install"
	},
	"dependencies": {
		"@types/node": "18.15.11",
		"@types/react": "18.0.34",
		"@types/react-dom": "18.0.11",
		"autoprefixer": "10.4.14",
		"axios": "^1.3.5",
		"classnames": "^2.3.2",
		"eslint": "8.38.0",
		"eslint-config-next": "^13.3.0",
		"husky": "^8.0.3",
		"next": "13.3.0",
		"next-auth": "^4.22.0",
		"postcss": "8.4.21",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.8.0",
		"recoil": "^0.7.7",
		"swr": "^2.1.3",
		"tailwindcss": "3.3.1",
		"typescript": "5.0.4"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"cspell": "^6.31.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-github": "^4.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"prettier": "^2.8.7",
		"prettier-plugin-tailwindcss": "^0.2.7",
		"rimraf": "^5.0.0",
		"sass": "^1.61.0"
	}
}
